{
	"singleQuote": true,
	"semi": false,
	"quoteProps": "consistent",
	"arrowParens": "avoid",
	"endOfLine": "lf",
	"trailingComma": "all",
	"printWidth": 100,
	"tabWidth": 2,
	"jsxSingleQuote": false,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
